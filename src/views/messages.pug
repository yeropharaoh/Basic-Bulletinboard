doctype html
html
	head
		style
			include style.css

		link(href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet")
		link(type="text/css" rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.1/css/materialize.min.css"  media="screen,projection")

		meta(name="viewport" content="width=device-width, initial-scale=1.0")
			
	body
		row.center
			h1 Here are the messages you posted!
			br
			p 
		
	each val in posts
		ul(class='postbox')
		
			li Title: #{val.title}
			li Message: #{val.body}
			//- li.header.col.s9.light.teal.lighten-1 Message: #{val.body}
	br
	br

	a#download-button.btn-large.waves-effect.waves-light.teal.lighten-1(href='/') Home
	|
	|

	br
	br

	a#download-button.btn-large.waves-effect.waves-light.teal.lighten-1(href='/messages') Messages
	|
	|


		

